<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:textStyle="bold"
        android:textSize="40px"
        android:text="Course ID: CSE 5330"
        android:ems="10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="55dp"
        android:id="@+id/viewAssignment_courseId" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:textStyle="bold"
        android:textSize="40px"
        android:text="Assignments:"
        android:ems="10"
        android:layout_marginTop="22dp"
        android:id="@+id/editText27"
        android:layout_below="@+id/viewAssignment_courseId"
        android:layout_alignStart="@+id/viewAssignment_courseId" />



    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText27"
        android:layout_marginTop="14dp"
        android:id="@+id/viewAssignment_RelativeLayout"
        android:layout_alignStart="@+id/editText27"
        android:layout_alignEnd="@+id/editText27" />


</RelativeLayout>